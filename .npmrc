# You can view the current config with `npm config list`

# Auth
//registry.npmjs.org/:_authToken=${NPM_TOKEN}

# Reliability
save-exact=true
auto-install-peers=true
package-lock=true
engine-strict=true

# UX
fund=false
update-notifier=false
loglevel=warn
progress=true

# Security
audit-level=moderate
audit=true
audit-signatures=true